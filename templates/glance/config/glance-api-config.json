      {
        "source": "/var/lib/config-data/merged/glance.conf.d",
        "dest": "/etc/glance/glance.conf.d",
        "owner": "glance",
        "perm": "0750"
      },
      {
        "source": "/var/lib/config-data/merged/glance-image-import.conf",
        "dest": "/etc/glance/glance.conf.d/glance-image-import.conf",
